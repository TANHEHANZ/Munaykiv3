generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id           String   @id @default(uuid())
  email        String   @unique
  emails       String[]
  name         String
  providerId   String   @unique
  providerType String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
